{
  "name": "musicoin-wallet",
  "description": "Official Musicoin desktop application.",
  "version": "1.5.0",
  "license": "MIT",
  "private": false,
  "node-main": "./hub/msc.js",
  "main": "./interface/index.html",
  "window": {
    "title": "Musicoin Desktop",
    "width": 1000,
    "min_width": 1000,
    "height": 600,
    "min_height": 600,
    "resizeable": true
  },
  "webkit": {
    "plugin": true,
    "page-cache": true
  },
  "build": {
    "appId": "org.musicoin.app.desktop",
    "nwVersion": "0.30.5",
    "nwFlavor": "normal",
    "files": [
      "**/*"
    ],
    "output": "./dist",
    "ffmpegIntegration": true,
    "targets": [
      "zip",
      "nsis7z"
    ],
    "mac": {
      "displayName": "Musicoin Desktop",
      "copyright": "Musicoin"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Musicoin/desktop.git"
  },
  "bugs": "https://github.com/Musicoin/desktop/issues",
  "dependencies": {
    "ethereum-blockies": "github:ethereum/blockies",
    "ethers": "~3.0.17",
    "fs-finder": "~1.8.1",
    "jsqr": "~1.0.4",
    "ntp-client": "",
    "pngjs": "~3.3.2",
    "qrcode": "~1.2.0",
    "request": "~2.87.0",
    "request-promise-native": "~1.0.5",
    "web3": "github:Musicoin/web3.js",
    "zxcvbn": "~4.4.2"
  },
  "scripts": {
    "build-all": "yarn --link-duplicates --force && yarn build-x64 && yarn build-x86",
    "build-x86": "yarn --link-duplicates --force && yarn run build -lw --x86 --concurrent ./",
    "build-x64": "yarn --link-duplicates --force && yarn run build -mlw --x64 --concurrent ./",
    "copy-gmc": "mkdir -p ./bin/gmc && cp -r ./node_modules/musicoin-daemon/bin/* ./bin/gmc/",
    "copy-ux": "cp -r ./node_modules/interface ./interface",
    "debug": "yarn --link-duplicates --force && yarn run run ./",
    "dev-cleanup": "rm -rf ./node_modules",
    "prepare": "rm -rf {./interface/package.json,./interface/node_modules}",
    "postinstall": "(yarn copy-gmc & yarn copy-ux)",
    "preinstall": "rm -rf {./bin,./dist,./interface}",
    "test": "ls -a dist/"
  },
  "devDependencies": {
    "@morhaus/nwjs-builder-phoenix": "~1.15.0",
    "interface": "github:Musicoin/desktop-interface",
    "musicoin-daemon": "github:Musicoin/gmc-node-modules"
  },
  "resolutions": {
    "inherits": "2.0.3",
    "uuid": "2.0.1",
    "async": "2.6.1",
    "yargs": "8.0.2",
    "camelcase": "4.1.0",
    "os-locale": "2.1.0",
    "read-pkg-up": "2.0.0",
    "string-width": "2.1.1",
    "which-module": "2.0.0",
    "yargs-parser": "7.0.0",
    "strip-ansi": "4.0.0",
    "is-fullwidth-code-point": "2.0.0",
    "ansi-regex": "3.0.0",
    "find-up": "2.1.0",
    "read-pkg": "2.0.0",
    "path-exists": "3.0.0",
    "load-json-file": "2.0.0",
    "path-type": "2.0.0",
    "strip-bom": "3.0.0",
    "tough-cookie": "2.4.2",
    "extsprintf": "1.4.0",
    "bluebird": "3.5.1",
    "colors": "1.3.0",
    "minimatch": "3.0.4"
  }
}
