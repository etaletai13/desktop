{
  "name": "musicoin-wallet",
  "description": "Official Musicoin desktop application.",
  "version": "1.5.0",
  "license": "MIT",
  "private": false,
  "node-main": "./hub/msc.js",
  "main": "./interface/index.html",
  "window": {
    "title": "Musicoin Desktop",
    "width": 1000,
    "min_width": 1000,
    "height": 600,
    "min_height": 600,
    "resizeable": true
  },
  "webkit": {
    "plugin": true,
    "page-cache": true
  },
  "build": {
    "appId": "org.musicoin.app.desktop",
    "nwVersion": "0.30.5",
    "nwFlavor": "normal",
    "output": "./dist",
    "ffmpegIntegration": true,
    "excludes": [
      "dist/",
      "local/",
      "config/config.ext.js",
      "favicon.png",
      "*.log",
      "*.md",
      ".yarn.lock",
      ".git",
      ".gitignore",
      ".snyk",
      ".travis.yml"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Musicoin/desktop.git"
  },
  "bugs": "https://github.com/Musicoin/desktop/issues",
  "dependencies": {
    "ethereum-blockies": "github:ethereum/blockies",
    "ethers": "~3.0.17",
    "fs-finder": "~1.8.1",
    "jsqr": "~1.0.4",
    "ntp-client": "",
    "pngjs": "~3.3.2",
    "qrcode": "~1.2.0",
    "request": "~2.87.0",
    "request-promise-native": "~1.0.5",
    "web3": "github:Musicoin/web3.js",
    "zxcvbn": "~4.4.2"
  },
  "scripts": {
    "build-x86": "yarn --link-duplicates --force && yarn run build -l --x86 ./ && yarn run build -w --x86 ./",
    "build-x64": "yarn --link-duplicates --force && yarn run build -m --x64 ./ && yarn run build -l --x64 ./ && yarn run build -w --x64 ./",
    "copy-gmc": "mkdir -p ./bin/gmc && cp -r ./node_modules/musicoin-daemon/bin/* ./bin/gmc/",
    "copy-ux": "cp -r ./node_modules/interface ./interface",
    "debug": "yarn --link-duplicates --force && yarn run run ./",
    "dev-cleanup": "rm -rf ./node_modules",
    "prepare": "rm -rf ./interface/package.json",
    "postinstall": "yarn copy-gmc && yarn copy-ux && rm -f ./interface/bower.json && rm -rf {./node_modules/interface,./node_modules/musicoin-daemon}",
    "preinstall": "rm -rf {./bin,./dist,./interface}",
    "test": "ls -a dist/"
  },
  "devDependencies": {
    "interface": "github:etaletai13/desktop-interface",
    "musicoin-daemon": "github:etaletai13/gmc-node-modules",
    "nwjs-builder-phoenix": "^1.14.6"
  }
}
